   164 000000CD 8B 44 24 10                     		MOV		EAX,[ESP+12+4]			; i = start;
   165 000000D1                                 mts_loop:
   166 000000D1 89 C3                           		MOV		EBX,EAX
   167 000000D3 81 C3 00000FFC                  		ADD		EBX,0xffc				; p = i + 0xffc;
   168 000000D9 8B 13                           		MOV		EDX,[EBX]				; old = *p;
   169 000000DB 89 33                           		MOV		[EBX],ESI				; *p = pat0;
   170 000000DD 83 33 FF                        		XOR		DWORD [EBX],0xffffffff	; *p ^= 0xffffffff;
   171 000000E0 3B 3B                           		CMP		EDI,[EBX]				; if (*p != pat1) goto fin;
   172 000000E2 75 18                           		JNE		mts_fin
