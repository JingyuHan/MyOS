   174 000000E7 3B 33                           		CMP		ESI,[EBX]				; if (*p != pat0) goto fin;
   175 000000E9 75 11                           		JNE		mts_fin
   176 000000EB 89 13                           		MOV		[EBX],EDX				; *p = old;
   177 000000ED 05 00001000                     		ADD		EAX,0x1000				; i += 0x1000;
